-- 這放在你的 A 檔案（啟動器）
local success, err = pcall(function()
    local targetURL = "https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/FLYUI"
    local rawData = game:HttpGet(targetURL)
    local func = loadstring(rawData)
    
    if func then
        func().Init()
    else
        error("無法解析腳本內容（可能是連結錯誤）")
    end
end)

if not success then
    warn("喵！載入 FLYUI 失敗: " .. tostring(err))
end
