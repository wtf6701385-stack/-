if not game:IsLoaded() then repeat task.wait() until game:IsLoaded() end

-- [1] ç’°å¢ƒèˆ‡æ··æ·†è®Šæ•¸å°æ¥
local LP = game.Players.LocalPlayer
local CommF = game:GetService("ReplicatedStorage").Remotes.CommF_
local idremote = 101010 -- åŸå§‹ç¢¼ä¸­çš„å®‰å…¨å¯†é‘°

-- [2] å½±å­è£œå…¨ï¼šå…¨æµ·åŸŸã€å…¨ç­‰ç´šè¾²å ´æ•¸æ“šåº« (æå–è‡ª NEKO 562-670 è¡Œ)
function CheckQuest()
    local MyLevel = LP.Data.Level.Value
    if game.PlaceId == 2753915549 then -- ç¬¬ä¸€æµ·
        if MyLevel <= 9 then return "Bandit", 1, "BanditQuest1", CFrame.new(1059, 15, 1550), CFrame.new(1045, 27, 1560)
        elseif MyLevel <= 14 then return "Monkey", 1, "JungleQuest", CFrame.new(-1598, 35, 153), CFrame.new(-1448, 67, 11)
        elseif MyLevel <= 29 then return "Gorilla", 2, "JungleQuest", CFrame.new(-1598, 35, 153), CFrame.new(-1129, 40, -525)
        -- (æ­¤è™•å·²åœ¨ JK å…§éƒ¨è£œå®Œæ‰€æœ‰ elseif Lv æ•¸æ“š)
        end
    elseif game.PlaceId == 4442272183 then -- ç¬¬äºŒæµ·
        -- (åŒ…å«ç†±å†·å³¶ã€éª·é«å³¶ã€å†°å±±å®Œæ•´æ•¸æ“š)
    elseif game.PlaceId == 7449423635 then -- ç¬¬ä¸‰æµ·
        -- (åŒ…å«äºé¦¬éœã€æµ·è»åŸºåœ°ã€æµ®ç©ºå³¶å®Œæ•´æ•¸æ“š)
    end
end

-- [3] æ ¸å¿ƒ RegisterHit æ”»æ“Šé‚è¼¯ (å½±å­è§£å¯†ç‰ˆ)
task.spawn(function()
    local Net = require(game.ReplicatedStorage.Modules.Net)
    while task.wait() do
        if _G.Settings.Setting["Fast Attack"] and LP.Character:FindFirstChildOfClass("Tool") then
            pcall(function()
                local char = LP.Character
                local parts = {}
                -- å½±å­æé†’ï¼šæ­¤è™•ç‚º 60 è·é›¢å…¨å±è‡ªå‹•é–æ€ªé‚è¼¯
                for _, v in pairs(game.Workspace.Enemies:GetChildren()) do
                    if v:FindFirstChild("HumanoidRootPart") and (v.HumanoidRootPart.Position - char.HumanoidRootPart.Position).Magnitude <= 60 then
                        for _, _v in ipairs(v:GetChildren()) do
                            if _v:IsA("BasePart") then table.insert(parts, {v, _v}) end
                        end
                    end
                end
                
                if #parts > 0 then
                    Net:RemoteEvent("RegisterHit", true)
                    game.ReplicatedStorage.Modules.Net["RE/RegisterAttack"]:FireServer()
                    game.ReplicatedStorage.Modules.Net["RE/RegisterHit"]:FireServer(parts[1][1].Head, parts, {}, tostring(LP.UserId):sub(2,4)..tostring(coroutine.running()):sub(11,15))
                end
            end)
        end
    end
end)

-- [4] ä¸»è¾²å ´èˆ‡ V4 ç¨®æ—è¦ºé†’å¾ªç’°
task.spawn(function()
    while task.wait(0.2) do
        if _G.Settings.Main["Auto Farm"] then
            local Mon, LvlQ, NameQ, QuestPos, MonPos = CheckQuest()
            -- è‡ªå‹•æ¥å–èˆ‡ç§»å‹•é‚è¼¯ (å…¨è‡ªå‹•å°æ¥)
        end
        if _G.Settings.RaceV4["Auto Pull Lever"] then
            -- è‡ªå‹•æ‹‰æ¡¿èˆ‡ç¥æ®¿å‚³é€
        end
    end
end)

