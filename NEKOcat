-- 1. å…ˆå«é–€è¡› A å‡ºä¾†å·¡é‚
local success, err = pcall(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/A?t="..tick()))()
end)

if not success then
    warn("âŒ è­¦å‘Šï¼šé–€è¡› A æ²’é–‹é–€ï¼Œè…³æœ¬çµ‚æ­¢ã€‚")
    return
end

-- 2. å•Ÿå‹•æ ¸å¿ƒåŠŸèƒ½
local function Authenticate()
    local Players = game:GetService("Players")
    local StarterGui = game:GetService("StarterGui")
    local RunService = game:GetService("RunService")
    local Camera = workspace.CurrentCamera
    local localPlayer = Players.LocalPlayer

    -- è¦–è§’ç¸®æ”¾è¨­å®šï¼š2000
    localPlayer.CameraMaxZoomDistance = 2000
    StarterGui:SetCore("SendNotification", {
        Title = " ğŸ± è²“è²“ç¸½éƒ¨ å•Ÿå‹• ",
        Text = " æ‰€æœ‰æ­¦è£å·²æº–å‚™å°±ç·’ï¼ ",
        Duration = 5
    })

    -- åŠ è¼‰ UI åº«
    local loadSuccess, NEKOLib = pcall(function()
        return loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOUI?t="..tick()))()
    end)
    if not loadSuccess or not NEKOLib then return end

    local Lib = NEKOLib:Init("à¸…^â€¢ï»Œâ€¢^à¸…")

    -- ç‹€æ…‹è®Šæ•¸
    _G.PVP, _G.MOB, _G.AFK, _G.Aimbot = false, false, false, false
    local Loaded = { PVP = false, MOB = false, SPD = false, FLY = false, ESP = false, V4 = false }

    -- [ è²“å…¬å‘Š ] --
    local Tab0 = Lib:CreateTab("è²“å…¬å‘Š", true) 
    Tab0:AddInfo("è²“è²“å…¬å‘Š", "V4 èˆ‡ é€è¦– å·²åŠ å…¥ï¼ç¥ä¸»äººç©å¾—é–‹å¿ƒï¼")

    -- [ è²“æˆ°é¬¥ ] --
    local Tab1 = Lib:CreateTab("è²“æˆ°é¬¥", false)
    
    Tab1:AddButton("æ‰“æ¶æ¨¡å¼ : OFF", function(self)
        _G.PVP = not _G.PVP
        if _G.PVP then
            _G.MOB = false
            if not Loaded.PVP then
                Loaded.PVP = true
                task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOPVP?t="..tick()))() end) end)
            end
        end
        self.Text = "  ğŸ± æ‰“æ¶æ¨¡å¼ : " .. (_G.PVP and "ON" or "OFF")
    end)

    -- âœ¨ æ–°å¢ï¼šå–µå–µV4
    Tab1:AddButton("ğŸ± å–µå–µV4", function()
        if not Loaded.V4 then
            Loaded.V4 = true
            task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOV4?t="..tick()))() end) end)
            StarterGui:SetCore("SendNotification", {Title = "NEKO", Text = "V4 è¦ºé†’ï¼", Duration = 2})
        end
    end)

    Tab1:AddButton("åˆ·æ€ªæ¨¡å¼ : OFF", function(self)
        _G.MOB = not _G.MOB
        if _G.MOB then
            _G.PVP = false
            if not Loaded.MOB then
                Loaded.MOB = true
                task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOMOB?t="..tick()))() end) end)
            end
        end
        self.Text = "  ğŸ± åˆ·æ€ªæ¨¡å¼ : " .. (_G.MOB and "ON" or "OFF")
    end)

    Tab1:AddButton("è²“é–é ­ : OFF", function(self)
        _G.Aimbot = not _G.Aimbot
        self.Text = "  ğŸ± è²“é–é ­ : " .. (_G.Aimbot and "ON" or "OFF")
    end)

    -- [ è²“åŠŸèƒ½ ] --
    local Tab2 = Lib:CreateTab("è²“åŠŸèƒ½", false)
    
    Tab2:AddButton("é˜²æ›æ©Ÿæ¨¡å¼ : OFF", function(self)
        _G.AFK = not _G.AFK
        self.Text = "  ğŸ± é˜²æ›æ©Ÿæ¨¡å¼ : " .. (_G.AFK and "ON" or "OFF")
    end)

    -- âœ¨ æ–°å¢ï¼šè²“è²“é€è¦–
    Tab2:AddButton("ğŸ± è²“è²“é€è¦–", function()
        if not Loaded.ESP then
            Loaded.ESP = true
            task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOESP?t="..tick()))() end) end)
        end
    end)

    Tab2:AddButton("ğŸ± è²“è²“ç¥é€Ÿ", function()
        if not Loaded.SPD then
            Loaded.SPD = true
            task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOSPD?t="..tick()))() end) end)
        end
    end)

    Tab2:AddButton("ğŸ± è²“è²“é£›è¡Œ", function()
        if not Loaded.FLY then
            Loaded.FLY = true
            task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOFly?t="..tick()))() end) end)
        end
    end)

    -- [ æ ¸å¿ƒå¾ªç’° ] --
    task.spawn(function()
        local VirtualUser = game:GetService("VirtualUser")
        RunService.RenderStepped:Connect(function()
            if _G.AFK then
                VirtualUser:CaptureController()
                VirtualUser:ClickButton2(Vector2.new())
            end
            if localPlayer.CameraMaxZoomDistance ~= 2000 then
                localPlayer.CameraMaxZoomDistance = 2000
            end
        end)
    end)
end

-- å•Ÿå‹•
Authenticate()
