if _G.NekoRunning then
    _G.NekoRunning = false
end

local function NuclearClean()
    local CoreGui = game:GetService("CoreGui")

    for _, name in ipairs({
        "è²“è²“è…³æœ¬",
        "NEKO",
        "NekoUI",
        "NekoTerminal_Lib"
    }) do
        local obj =
            CoreGui:FindFirstChild(name) or
            localPlayer.PlayerGui:FindFirstChild(name)

        if obj then
            obj:Destroy()
        end
    end
end

NuclearClean()
task.wait(0.2)

