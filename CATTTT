local TabTP = Window:Tab("å‚³é€")
local SecTP = TabTP:Section("è·¨æµ·å‚³é€")

local function Sea(cmd)
    pcall(function()
        ReplicatedStorage.Remotes.CommF_:InvokeServer(cmd)
    end)
end

