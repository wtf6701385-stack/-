        pcall(function()
            L_User.Text   = "ğŸ‘¤ ç©å®¶ï¼š" .. LocalPlayer.Name
            L_Attack.Text = "âš”ï¸ æ”»æ“Šï¼š" .. (_G.NEKO_STATUS.Attack and "ON" or "OFF")
