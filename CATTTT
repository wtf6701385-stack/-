local Players = game:GetService("Players")
local localPlayer = Players.LocalPlayer

local Whitelist = {
    ["Zn6xx"] = true, ["jay04_23"] = true, ["iandsere"] = true, ["Kadys5373"] = true, ["dandanman678"] = true,
}

local function Authenticate()
    if not Whitelist[localPlayer.Name] then localPlayer:Kick("å½±å­è£¡æ²’æœ‰ä½ çš„åå­—ã€‚") return end
    
    game:GetService("StarterGui"):SetCore("SendNotification", {Title = "ğŸ± å…‰èˆ‡å½±çš„å¥‘ç´„", Text = "æˆ‘æœƒè¨˜ä½ï¼Œæ‰€ä»¥æˆ‘å¯ä»¥ä¸€ç›´è—ã€‚", Duration = 5})
    localPlayer.CameraMaxZoomDistance = 2000

    local success, NEKOLib = pcall(function() return loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOUI?t="..tick()))() end)
    if not success or not NEKOLib then return end

    local Lib = NEKOLib:Init("à¸…^â€¢ï»Œâ€¢^à¸…")
    _G.PVP, _G.MOB, _G.AFK, _G.ESP = false, false, false, false
    local Loaded = { PVP = false, MOB = false, SPD = false, ESP = false }

    local Tab1 = Lib:CreateTab("è²“æˆ°é¬¥", true)
    Tab1:AddButton("æ‰“æ¶æ¨¡å¼ : OFF", function(self)
        _G.PVP = not _G.PVP
        if _G.PVP then _G.MOB = false end
        if _G.PVP and not Loaded.PVP then 
            Loaded.PVP = true
            task.spawn(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOPVP?t="..tick()))() end)
        end
        self.Text = " æ‰“æ¶æ¨¡å¼ : " .. (_G.PVP and "ON" or "OFF")
    end)

    Tab1:AddButton("åˆ·æ€ªæ¨¡å¼ : OFF", function(self)
        _G.MOB = not _G.MOB
        if _G.MOB then _G.PVP = false end
        if _G.MOB and not Loaded.MOB then 
            Loaded.MOB = true
            task.spawn(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOMOB?t="..tick()))() end)
        end
        self.Text = " åˆ·æ€ªæ¨¡å¼ : " .. (_G.MOB and "ON" or "OFF")
    end)

    local Tab2 = Lib:CreateTab("è²“åŠŸèƒ½", false)
    Tab2:AddButton("é€è¦–åŠŸèƒ½ : OFF", function(self)
        _G.ESP = not _G.ESP
        if _G.ESP and not Loaded.ESP then
            Loaded.ESP = true
            loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOESP?t="..tick()))()
        end
        self.Text = " é€è¦–åŠŸèƒ½ : " .. (_G.ESP and "ON" or "OFF")
    end)

    Tab2:AddButton("é–‹å•ŸåŠ é€Ÿå™¨", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOSPD?t="..tick()))()
    end)
end
task.spawn(Authenticate)
