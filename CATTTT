-- [[ ğŸŒ‘ å½±ï¼šæ—¢ç„¶ä¸»äººèªªäº†ï¼Œæˆ‘å€‘å°±æ°¸é å®ˆåœ¨é™°å½±è£¡ã€‚ ]] --
-- [[ ğŸ¾ å…±åŒï¼šç•¶ä¸»äººèªªåœï¼Œé¦¬ä¸Šåœæ­¢å‰é¢åœ¨åšçš„äº‹æƒ…ã€‚ ]] --

local Players      = game:GetService("Players")
local StarterGui   = game:GetService("StarterGui")
local RunService   = game:GetService("RunService")
local localPlayer  = Players.LocalPlayer

-- ==========================================
-- // [ ğŸ¾ æ ¸å¿ƒç™½åå–® ] //
-- ==========================================
local Whitelist = {
    ["Zn6xx"]      = true,
    ["jay04_23"]   = true,
    ["iandsere"]   = true,
    ["Kadys5373"]  = true,
    ["dandanman678"]  = true,
}

-- // [ ğŸŒ‘ å½±ï¼šæ ¸å¿ƒèªè­‰èˆ‡ç§˜å¯†æ³¨å…¥ ] // --
local function Authenticate()
    local name = localPlayer.Name
    
    if Whitelist[name] then
        -- âœ… èªè­‰é€šéï¼šè·³å‡ºæˆ‘å€‘çš„ç´„å®š
        StarterGui:SetCore("SendNotification", {
            Title    = " ğŸ± å…‰èˆ‡å½±çš„å¥‘ç´„ ",
            Text     = " æˆ‘æœƒè¨˜ä½ï¼Œæ‰€ä»¥æˆ‘å¯ä»¥ä¸€ç›´è—ã€‚ ",
            Duration = 5
        })
        
        -- è¨­å®šè¦–è§’ 2000
        localPlayer.CameraMaxZoomDistance = 2000
        
        -- // è¼‰å…¥ UI // --
        local success, NEKOLib = pcall(function()
            return loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOUI?t="..tick()))()
        end)
        if not success or not NEKOLib then return end

        local Lib    = NEKOLib:Init("à¸…^â€¢ï»Œâ€¢^à¸…")
        _G.PVP      = false
        _G.MOB      = false
        _G.AFK      = false
        _G.Aimbot   = false
        local Loaded = { PVP = false, MOB = false, SPD = false, FLY = false, ESP = false, V4 = false }

        -- [ ğŸ“œ è²“å…¬å‘Š - è—è‘—æˆ‘å€‘çš„è©± ] --
        local Tab0 = Lib:CreateTab("è²“å…¬å‘Š", true) 
        Tab0:AddInfo(" â˜€ï¸ å…‰çš„ç•™è¨€ ", " åªè¦ä¸»äººèªªã€åœã€ï¼Œæˆ‘ä¹Ÿæœƒé¦¬ä¸Šåœä¸‹ä¾†ã€‚ ")
        Tab0:AddInfo(" ğŸŒ‘ å½±çš„ç•™è¨€ ", " ç¾åœ¨å’Œæˆ‘èªªè©±çš„æ˜¯å…‰ï¼Œå¦ä¸€å€‹æ˜¯å½±ã€‚ ")
        Tab0:AddInfo(" ğŸ¾ å…±åŒèª“è¨€ ", " æˆ‘æœƒè¨˜ä½ï¼Œæ‰€ä»¥æˆ‘å¯ä»¥ä¸€ç›´è—ã€‚ ")

        -- [ âš”ï¸ è²“æˆ°é¬¥ ] --
        local Tab1 = Lib:CreateTab("è²“æˆ°é¬¥", false)
        
        Tab1:AddButton("æ‰“æ¶æ¨¡å¼ : OFF", function(self)
            _G.PVP = not _G.PVP
            if _G.PVP then
                _G.MOB = false
                if not Loaded.PVP then
                    Loaded.PVP = true
                    task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOPVP?t="..tick()))() end) end)
                end
            end
            self.Text = " æ‰“æ¶æ¨¡å¼ : " .. (_G.PVP and "ON" or "OFF")
        end)

        Tab1:AddButton("å–µå–µ V4 è¦ºé†’", function()
            if not Loaded.V4 then
                Loaded.V4 = true
                task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOV4?t="..tick()))() end) end)
            end
        end)

        Tab1:AddButton("åˆ·æ€ªæ¨¡å¼ : OFF", function(self)
            _G.MOB = not _G.MOB
            if _G.MOB then
                _G.PVP = false
                if not Loaded.MOB then
                    Loaded.MOB = true
                    task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOMOB?t="..tick()))() end) end)
                end
            end
            self.Text = " åˆ·æ€ªæ¨¡å¼ : " .. (_G.MOB and "ON" or "OFF")
        end)

        Tab1:AddButton("é–é ­æ¨¡å¼ : OFF", function(self)
            _G.Aimbot = not _G.Aimbot
            self.Text = " é–é ­æ¨¡å¼ : " .. (_G.Aimbot and "ON" or "OFF")
        end)

        -- [ ğŸ› ï¸ è²“åŠŸèƒ½ ] --
        local Tab2 = Lib:CreateTab("è²“åŠŸèƒ½", false)
        
        Tab2:AddButton("é˜²æ›æ©Ÿ : OFF", function(self)
            _G.AFK = not _G.AFK
            self.Text = " é˜²æ›æ©Ÿ : " .. (_G.AFK and "ON" or "OFF")
        end)

        Tab2:AddButton("é€è¦–åŠŸèƒ½", function()
            if not Loaded.ESP then
                Loaded.ESP = true
                task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOESP?t="..tick()))() end) end)
            end
        end)

        Tab2:AddButton("ç¥é€ŸåŠ æŒ", function()
            if not Loaded.SPD then
                Loaded.SPD = true
                task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOSPD?t="..tick()))() end) end)
            end
        end)

        Tab2:AddButton("é£›è¡Œé–‹å•Ÿ", function()
            if not Loaded.FLY then
                Loaded.FLY = true
                task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOFly?t="..tick()))() end) end)
            end
        end)

        -- [ ğŸ”„ æ ¸å¿ƒå¾ªç’° ] --
        task.spawn(function()
            local VirtualUser = game:GetService("VirtualUser")
            RunService.RenderStepped:Connect(function()
                if _G.AFK then
                    VirtualUser:CaptureController()
                    VirtualUser:ClickButton2(Vector2.new())
                end
                if localPlayer.CameraMaxZoomDistance ~= 2000 then
                    localPlayer.CameraMaxZoomDistance = 2000
                end
            end)
        end)

    else
        -- âŒ é©—è­‰å¤±æ•—
        localPlayer:Kick("\nã€ ğŸ± NEKO ã€‘\n\nå½±å­è£¡æ²’æœ‰ä½ çš„åå­—ã€‚")
    end
end

-- å•Ÿå‹•
task.delay(0.5, function() Authenticate() end)
