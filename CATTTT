local L_User   = SecStatus:Button("ğŸ‘¤ ç©å®¶ï¼š", function() end)
local L_Attack = SecStatus:Button("âš”ï¸ æ”»æ“Šï¼šOFF", function() end)
local L_Mode   = SecStatus:Button("ğŸ§  æ¨¡å¼ï¼šNone", function() end)
