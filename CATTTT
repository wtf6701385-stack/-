local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOUI"))()
local myUI = library.new("ğŸ¾ NEKO UI")

-- [[ 2. æ ¸å¿ƒæ•¸æ“š ]] --
local Settings = { Dist = 1500, Mode = "None", Pull = false }
_G.Atk = false; _G.V4 = false

-- [[ 3. åŠŸèƒ½å°æ¥ (åš´æ ¼æŒ‰ç…§ä½ çš„ UI çµæ§‹) ]] --

-- åˆ†é ä¸€ï¼šæˆ°é¬¥ä¸­å¿ƒ
local Tab1 = myUI.Tab("âš”ï¸ æˆ°é¬¥ä¸­å¿ƒ")
local SecCombat = Tab1.Section("ç¥åˆ€ç‹©çµç³»çµ±")

-- é€™è£¡ç›´æ¥èª¿ç”¨ section.Toggle å’Œ section.Button
SecCombat.Toggle("è‡ªå‹•ç¥åˆ€é€£æ“Š", function(v) _G.Atk = v end)
SecCombat.Toggle("è‡ªå‹•è²“æŠ“æ‹‰æ€ª", function(v) Settings.Pull = v end)
SecCombat.Toggle("è‡ªå‹•è¦ºé†’ (V4)", function(v) _G.V4 = v end)

local SecMode = Tab1.Section("æ¨¡å¼åˆ‡æ›")
SecMode.Button("æ¨¡å¼ï¼šåƒ…æ‰“å°æ€ª", function() Settings.Mode = "Mob" end)
SecMode.Button("æ¨¡å¼ï¼šå…¨å ´ç©å®¶", function() Settings.Mode = "All" end)
SecMode.Button("æ¨¡å¼ï¼šåœæ­¢é‹è¡Œ", function() Settings.Mode = "None" end)

-- åˆ†é äºŒï¼šèº«æ³•è¦–è¨Š
local Tab2 = myUI.Tab("ğŸš€ è¼”åŠ©åŠŸèƒ½")
local SecMove = Tab2.Section("èº«æ³•èˆ‡é€è¦–")

SecMove.Button("åŠ è¼‰æ¥µè‡´è²“é€Ÿ", function() 
    loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOSPD"))() 
end)
SecMove.Button("åŠ è¼‰è¼•åŠŸé£›è¡Œ", function() 
    loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOFly"))() 
end)
SecMove.Button("é–‹å•Ÿè²“çœ¼ (ESP)", function() 
    loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOESP"))() 
