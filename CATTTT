-- ==========================================
-- ã€ â¤ï¸ 1. æ¥µé€Ÿç™½åå–®åˆ¤å®š ã€‘
-- ==========================================
local Whitelist = { 
    ["Kadys5373"] = true, ["jay04_23"] = true, ["Zn6xx"] = true, 
    ["iandsere"] = true, ["Zadrian664"] = true, ["czr_0416"] = true, 
    ["tw_vic2012"] = true, ["12i21777"] = true
