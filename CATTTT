pcall(function()
    local CoreGui = game:GetService("CoreGui")
    for _, n in ipairs({"è²“è²“è…³æœ¬","NEKO","NekoUI"}) do
        local g = CoreGui:FindFirstChild(n)
        if g then g:Destroy() end
    end
end)

