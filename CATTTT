local TabFight = myUI:Tab("æˆ°é¬¥ç³»çµ±")

-- ğŸ“ æ™ºèƒ½è¿½è¹¤æ¨¡çµ„ (å¬å–šé‚è¼¯)
local S1 = TabFight:Section("æ™ºèƒ½è¿½è¹¤ (Sticky)")

local function GetNearestPlayer()
    local target, dist = nil, 2000
    for _, p in ipairs(Players:GetPlayers()) do
        if p ~= localPlayer and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
            local d = localPlayer:DistanceFromCharacter(p.Character.HumanoidRootPart.Position)
            if d < dist then dist = d; target = p end
        end
    end
    return target
end

S1:Toggle("å¬å–šé»äºº (é–å®šæœ€è¿‘)", function(s)
    _G.Settings.StickyEnabled = s
    if s then
        _G.Settings.StickyTarget = GetNearestPlayer()
    else
        _G.Settings.StickyTarget = nil
    end
end)

S1:Button("ğŸ”„ é‡æ–°å¬å–š (æ›äººé»)", function()
    if _G.Settings.StickyEnabled then
        _G.Settings.StickyTarget = GetNearestPlayer()
    end
end)

-- âš”ï¸ åŸæœ‰ç¥åˆ€åŠŸèƒ½
