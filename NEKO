-- --- [[ 4. å»ºç«‹ Tab1ï¼šè²“æˆ°é¬¥ ]] --- --
local Tab1 = Lib:CreateTab("è²“æˆ°é¬¥", false)

Tab1:AddButton("æ‰“æ¶æ¨¡å¼ : OFF", function(self)
    _G.PVP = not _G.PVP
    if _G.PVP then
        -- å•Ÿå‹•æ™‚æ‰ loadstring
        task.spawn(function()
            pcall(function() 
                loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOPVP?t="..tick()))() 
            end)
        end)
    end
    self.Text = "  ğŸ± æ‰“æ¶æ¨¡å¼ : " .. (_G.PVP and "ON" or "OFF")
end)

Tab1:AddButton("åˆ·æ€ªæ¨¡å¼ : OFF", function(self)
