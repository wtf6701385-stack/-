-- 4. è²“å…¬å‘Šå…§å®¹
CatLib:CreateInfo(p0, " ğŸ¾ "," æ­¡è¿ä½¿ç”¨è²“è²“è…³æœ¬ ")
CatLib:CreateInfo(p0, " ğŸ¾ ", " ä¸è¦é»å¤ªå¿«è¬è¬æ‚¨ ")

-- 5. è²“åŠŸèƒ½åˆ†é  (åŒ…å« PVP/MOB/ESP)
local btnPVP, btnMOB

btnPVP = CatLib:CreateButton(p1, "æ‰“æ¶æ¨¡å¼ : OFF", function(self)
    _G.PVP = not _G.PVP
    if _G.PVP then 
        _G.MOB = false 
        if btnMOB then btnMOB.Text = "  ğŸ± åˆ·æ€ªæ¨¡å¼ : OFF" end
        loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOPVP"))()
    end
    self.Text = "  ğŸ± æ‰“æ¶æ¨¡å¼ : "..(_G.PVP and "ON" or "OFF")
end)

btnMOB = CatLib:CreateButton(p1, "åˆ·æ€ªæ¨¡å¼ : OFF", function(self)
    _G.MOB = not _G.MOB
    if _G.MOB then 
        _G.PVP = false 
        if btnPVP then btnPVP.Text = "  ğŸ± æ‰“æ¶æ¨¡å¼ : OFF" end
        loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOMOB"))()
    end
    self.Text = "  ğŸ± åˆ·æ€ªæ¨¡å¼ : "..(_G.MOB and "ON" or "OFF")
end)

CatLib:CreateButton(p1, "é€è¦–å°æ‰‹ : OFF", function(self)
