-- --- [[ 4. å»ºç«‹ Tab1ï¼šè²“æˆ°é¬¥ ]] --- --
local Tab1 = Lib:CreateTab("è²“æˆ°é¬¥", false)
local btnPVP, btnMOB

btnPVP = Tab1:AddButton("æ‰“æ¶æ¨¡å¼ : OFF", function(self)
    _G.PVP = not _G.PVP
    if _G.PVP then
        _G.MOB = false
        if btnMOB then btnMOB.Text = "  ğŸ± åˆ·æ€ªæ¨¡å¼ : OFF" end
        -- é–‹å•Ÿæ™‚ç›´æ¥ loadstringï¼Œè®“å­è…³æœ¬çš„ ID æ©Ÿåˆ¶è™•ç†é‡è¤‡å•é¡Œ
        task.spawn(function() 
            pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOPVP?t="..tick()))() end) 
        end)
    end
    self.Text = "  ğŸ± æ‰“æ¶æ¨¡å¼ : " .. (_G.PVP and "ON" or "OFF")
end)

btnMOB = Tab1:AddButton("åˆ·æ€ªæ¨¡å¼ : OFF", function(self)
    _G.MOB = not _G.MOB
    if _G.MOB then
        _G.PVP = false
        if btnPVP then btnPVP.Text = "  ğŸ± æ‰“æ¶æ¨¡å¼ : OFF" end
