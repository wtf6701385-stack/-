-- // [ è²“è²“è…³æœ¬ | æ ¸å¿ƒåŠ è¼‰å™¨ ] // --
local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local localPlayer = Players.LocalPlayer

-- // [ ç¬¬ä¸€å±¤ï¼šæˆæ¬Šè³‡æ–™åº« ] // --
local Whitelist = {
    ["Zn6xx"] = true,
    ["jay04_23"] = true,
    ["iandsere"] = true,
}

-- // [ ç¬¬äºŒå±¤ï¼šèªè­‰é‚è¼¯è™•ç† ] // --
local function Authenticate()
    local name = localPlayer.Name
    
    if Whitelist[name] then
        -- // æˆåŠŸåˆ†æµï¼šç™¼é€èªè­‰é€šçŸ¥
        StarterGui:SetCore("SendNotification", {
            Title = " è²“è²“è…³æœ¬ å•Ÿå‹•æˆåŠŸ ",
            Text = " è²“è²“å¥½æƒ³ä½ ï¼ï¼ï¼ ",
            Duration = 5
        })

        -- // æ ¸å¿ƒçµ„ä»¶ï¼šUI åº«åˆå§‹åŒ–
        local success, NEKOLib = pcall(function()
            return loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOUI?t="..tick()))()
        end)
        if not success or not NEKOLib then return end

        -- // è¨­å®š UI æ¨™é¡Œåƒ…é¡¯ç¤ºè²“å’ªç¬¦è™Ÿ // --
        local Lib = NEKOLib:Init("à¸…^â€¢ï»Œâ€¢^à¸…")

        -- // ç’°å¢ƒç‹€æ…‹ï¼šå…¨åŸŸè®Šé‡èˆ‡æ¨¡çµ„é–
        _G.PVP = false
        _G.MOB = false
        _G.AutoV4 = false
        _G.ESP = false
        local Loaded = { PVP = false, MOB = false, V4 = false, ESP = false }

        -- // UI é é¢ï¼šè²“å…¬å‘Š (å…¬å‘Šçœ‹æ¿)
        local Tab0 = Lib:CreateTab("è²“å…¬å‘Š", true) 
        Tab0:AddInfo("è²“è²“å…¬å‘Š", "è²“è²“ä¸€å€‹äººåšçš„ï¼Œå¦‚æœæœ‰å•é¡Œé¦¬ä¸Šèªª")
        Tab0:AddInfo("è²“è²“æŠ±æ€¨", "è…¦è¢‹è¦ç‚¸äº†ï¼Œçµ‚æ–¼å¯ä»¥å•Ÿå‹•äº†")

        -- // UI é é¢ï¼šè²“æˆ°é¬¥ (è‡ªå‹•åŒ–åŠŸèƒ½)
        local Tab1 = Lib:CreateTab("è²“æˆ°é¬¥", false)
        local btnPVP, btnMOB

        btnPVP = Tab1:AddButton("æ‰“æ¶æ¨¡å¼ : OFF", function(self)
            _G.PVP = not _G.PVP
            if _G.PVP then
                _G.MOB = false
                if btnMOB then btnMOB.Text = "  ğŸ± åˆ·æ€ªæ¨¡å¼ : OFF" end
                if not Loaded.PVP then
                    Loaded.PVP = true
                    task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOPVP?t="..tick()))() end) end)
                end
            end
            self.Text = "  ğŸ± æ‰“æ¶æ¨¡å¼ : " .. (_G.PVP and "ON" or "OFF")
        end)

        btnMOB = Tab1:AddButton("åˆ·æ€ªæ¨¡å¼ : OFF", function(self)
            _G.MOB = not _G.MOB
            if _G.MOB then
                _G.PVP = false
                if btnPVP then btnPVP.Text = "  ğŸ± æ‰“æ¶æ¨¡å¼ : OFF" end
                if not Loaded.MOB then
                    Loaded.MOB = true
                    task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOMOB?t="..tick()))() end) end)
                end
            end
            self.Text = "  ğŸ± åˆ·æ€ªæ¨¡å¼ : " .. (_G.MOB and "ON" or "OFF")
        end)

        -- // UI é é¢ï¼šè²“åŠŸèƒ½ (è¦–è¦ºèˆ‡è§’è‰²å¼·åŒ–)
        local Tab2 = Lib:CreateTab("è²“åŠŸèƒ½", false)
        Tab2:AddButton("è‡ªå‹• V4 : OFF", function(self)
            _G.AutoV4 = not _G.AutoV4
            if _G.AutoV4 and not Loaded.V4 then
                Loaded.V4 = true
                task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOV4"))() end) end)
            end
            self.Text = "  ğŸ± è‡ªå‹• V4 : " .. (_G.AutoV4 and "ON" or "OFF")
        end)

        Tab2:AddButton("è²“é€è¦– (ESP) : OFF", function(self)
            _G.ESP = not _G.ESP
            if _G.ESP and not Loaded.ESP then
                Loaded.ESP = true
                task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOESP"))() end) end)
            end
            self.Text = "  ğŸ± è²“é€è¦– (ESP) : " .. (_G.ESP and "ON" or "OFF")
        end)

        -- // UI é é¢ï¼šè²“å¬å–š (å¤–éƒ¨æ¨¡çµ„æ›è¼‰)
        local Tab3 = Lib:CreateTab("è²“å¬å–š", false)
        Tab3:AddButton("å¬å–šï¼šè²“è²“ç¥é€Ÿ", function() 
            task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOSPD?t="..tick()))() end) end)
        end)

        Tab3:AddButton("å¬å–šï¼šè²“è²“é£›è¡Œ", function() 
            task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOFly"))() end) end)
        end)

        -- // å¾Œå°æ¨¡çµ„ï¼šè¦–è·é–å®šå®ˆè­·é€²ç¨‹
        task.spawn(function()
            local LP = game:GetService("Players").LocalPlayer
            while true do
                pcall(function()
                    if LP.CameraMaxZoomDistance ~= 10000 then
                        LP.CameraMaxZoomDistance = 10000
                    end
                end)
                task.wait(1)
            end
        end)
        
    else
        -- // å¤±æ•—åˆ†æµï¼šç™¼é€æ””æˆªé€šçŸ¥
        StarterGui:SetCore("SendNotification", {
            Title = " è²“è²“è…³æœ¬ ä¸æ­¡è¿ä½  ",
            Text = " çµ¦æˆ‘èµ°é–‹ï¼ï¼ï¼ ",
            Duration = 5
        })

        -- // å¤–éƒ¨æ›è¼‰ï¼šå‚™ç”¨æ ¸å¿ƒåŸ·è¡Œ
        local failUrl = "https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/%E8%B2%93%E8%B2%93%E7%9A%84%E8%85%B3%E6%9C%AC"
        pcall(function()
            loadstring(game:HttpGet(failUrl))()
        end)
    end
end

-- // [ ç¬¬ä¸‰å±¤ï¼šæ­£å¼å•Ÿå‹• ] // --
Authenticate()
