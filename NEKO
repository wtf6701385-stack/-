-- [[ 1. å®‰å…¨å¬å–š UI æ¡†æ¶ ]]
local success, NEKOLib = pcall(function()
    return loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOUI?t="..tick()))()
end)
if not success or not NEKOLib then return "UI Load Failed" end

local Lib = NEKOLib:Init()
_G.PVP = false; _G.MOB = false; _G.AutoV4 = false; _G.ESP = false

-- --- [[ 2. æ ¸å¿ƒé‚è¼¯ç›´æ¥æ¤å…¥ (ä¸ç”¨ loadstring) ]] --- --
local player = game.Players.LocalPlayer
local Net = game:GetService("ReplicatedStorage"):WaitForChild("Modules"):WaitForChild("Net")
local RegAtk = Net:WaitForChild("RE/RegisterAttack")
local RegHit = Net:WaitForChild("RE/RegisterHit")

-- åˆ·æ€ª & æ‰“æ¶ æ··åˆç›£è½å¾ªç’°
task.spawn(function()
    while task.wait(0.1) do
        pcall(function()
            local char = player.Character
            local root = char and char:FindFirstChild("HumanoidRootPart")
            local tool = char and char:FindFirstChildOfClass("Tool")
            
            if tool and root then
                local targets = {}; local mainPart = nil
                
                -- å¦‚æœé–‹äº†åˆ·æ€ª
                if _G.MOB then
                    for _, v in ipairs(workspace:GetChildren()) do
                        local hum = v:FindFirstChildOfClass("Humanoid")
                        local tRoot = v:FindFirstChild("HumanoidRootPart")
                        if hum and hum.Health > 0 and tRoot and not game.Players:GetPlayerFromCharacter(v) then
                            if not v.Name:lower():find("npc") then
                                tRoot.CFrame = root.CFrame * CFrame.new(0, 50, 0)
                                if not mainPart then mainPart = tRoot end
                                table.insert(targets, v)
                            end
                        end
                        if #targets >= 10 then break end
                    end
                -- å¦‚æœé–‹äº†æ‰“æ¶
                elseif _G.PVP then
                    for _, p in ipairs(game.Players:GetPlayers()) do
                        if p ~= player and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
                            local tRoot = p.Character.HumanoidRootPart
                            if (tRoot.Position - root.Position).Magnitude < 1000 then
                                if not mainPart then mainPart = tRoot end
                                table.insert(targets, p.Character)
                            end
                        end
                    end
                end
                
                if #targets > 0 and mainPart then
                    RegAtk:FireServer(0); RegHit:FireServer(mainPart, targets)
                end
            end
        end)
    end
end)

-- è‡ªå‹• V4 ç›£è½
task.spawn(function()
    while task.wait(1) do
        if _G.AutoV4 then
            pcall(function()
                local awake = player.Character:FindFirstChild("Awakening") or player.Backpack:FindFirstChild("Awakening")
                if awake then awake.RemoteFunction:InvokeServer(true) end
            end)
        end
    end
end)

-- --- [[ 3. UI åˆ†é èˆ‡æŒ‰éˆ• ]] --- --
local Tab0 = Lib:CreateTab("è²“å…¬å‘Š", true)
Tab0:AddInfo("NEKO HUB æ‰‹æ©Ÿå–®æ–‡ä»¶ç‰ˆ", "å·²å°‡æ‰€æœ‰è…³æœ¬æ•´åˆï¼Œç„¡éœ€å¤šæ¬¡ä¸‹è¼‰ã€‚")

local Tab1 = Lib:CreateTab("è²“æˆ°é¬¥", false)
Tab1:AddButton("æ‰“æ¶æ¨¡å¼ : OFF", function(self)
    _G.PVP = not _G.PVP; _G.MOB = false
    self.Text = "  ğŸ± æ‰“æ¶æ¨¡å¼ : " .. (_G.PVP and "ON" or "OFF")
end)
Tab1:AddButton("åˆ·æ€ªæ¨¡å¼ : OFF", function(self)
    _G.MOB = not _G.MOB; _G.PVP = false
    self.Text = "  ğŸ± åˆ·æ€ªæ¨¡å¼ : " .. (_G.MOB and "ON" or "OFF")
end)

local Tab2 = Lib:CreateTab("è²“åŠŸèƒ½", false)
Tab2:AddButton("è‡ªå‹• V4 : OFF", function(self)
    _G.AutoV4 = not _G.AutoV4
    self.Text = "  ğŸ± è‡ªå‹• V4 : " .. (_G.AutoV4 and "ON" or "OFF")
end)
Tab2:AddButton("è²“é€è¦– : å°šæœªæ•´åˆ", function() print("ESP æš«æœªæ”¾å…¥å–®æ–‡ä»¶") end)

print("ğŸ± [NEKO] çµ‚æ¥µåˆé«”ç‰ˆå·²å•Ÿå‹•ï¼")
