-- [[ 1. å®‰å…¨å¬å–š UI æ¡†æ¶ ]]
local success, NEKOLib = pcall(function()
    return loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOUI?t="..tick()))()
end)
if not success or not NEKOLib then return end

local Lib = NEKOLib:Init()

-- [[ 2. å…¨åŸŸç‹€æ…‹èˆ‡åŠ è¼‰é– ]]
_G.PVP = false
_G.MOB = false
_G.AutoV4 = false
_G.ESP = false
local Loaded = { PVP = false, MOB = false, V4 = false, ESP = false }

-- --- [[ 3. å»ºç«‹ Tab0ï¼šè²“å…¬å‘Š ]] --- --
-- æ³¨æ„ï¼šé€™è£¡ç¬¬äºŒå€‹åƒæ•¸è¨­ç‚º trueï¼Œç¢ºä¿ UI æ‰“é–‹ç¬¬ä¸€çœ¼å°±çœ‹åˆ°å®ƒ
local Tab0 = Lib:CreateTab("è²“å…¬å‘Š", true) 
Tab0:AddInfo("NEKO HUB ç³»çµ±å…¬å‘Š", "æ­¡è¿ä½¿ç”¨æ ¡æº–ç‰ˆï¼ŒåŠŸèƒ½å·²å…¨é¢å°æ¥é–‹é—œã€‚")
Tab0:AddInfo("æ›´æ–°æ—¥èªŒ", "12/28: ä¿®å¾©åˆ†é æ¸²æŸ“å¤±æ•—èˆ‡æŒ‰éˆ•ä¸é¡¯ç¤ºå•é¡Œã€‚")

-- --- [[ 4. å»ºç«‹ Tab1ï¼šè²“æˆ°é¬¥ ]] --- --
local Tab1 = Lib:CreateTab("è²“æˆ°é¬¥", false)
local btnPVP, btnMOB

btnPVP = Tab1:AddButton("æ‰“æ¶æ¨¡å¼ : OFF", function(self)
    _G.PVP = not _G.PVP
    if _G.PVP then
        _G.MOB = false
        if btnMOB then btnMOB.Text = "  ğŸ± åˆ·æ€ªæ¨¡å¼ : OFF" end -- æ³¨æ„ï¼šé€™è£¡è¦æ‰‹å‹•åŠ  ğŸ± å› ç‚º UI åº«åªæœ‰åˆæ¬¡ç”Ÿæˆæ‰åŠ 
        if not Loaded.PVP then
            Loaded.PVP = true
            task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOPVP?t="..tick()))() end) end)
        end
    end
    self.Text = "  ğŸ± æ‰“æ¶æ¨¡å¼ : " .. (_G.PVP and "ON" or "OFF")
end)

btnMOB = Tab1:AddButton("åˆ·æ€ªæ¨¡å¼ : OFF", function(self)
    _G.MOB = not _G.MOB
    if _G.MOB then
        _G.PVP = false
        if btnPVP then btnPVP.Text = "  ğŸ± æ‰“æ¶æ¨¡å¼ : OFF" end
        if not Loaded.MOB then
            Loaded.MOB = true
            task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOMOB?t="..tick()))() end) end)
        end
    end
    self.Text = "  ğŸ± åˆ·æ€ªæ¨¡å¼ : " .. (_G.MOB and "ON" or "OFF")
end)

-- --- [[ 5. å»ºç«‹ Tab2ï¼šè²“åŠŸèƒ½ ]] --- --
local Tab2 = Lib:CreateTab("è²“åŠŸèƒ½", false)

Tab2:AddButton("è‡ªå‹• V4 : OFF", function(self)
    _G.AutoV4 = not _G.AutoV4
    if _G.AutoV4 and not Loaded.V4 then
        Loaded.V4 = true
        task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOV4"))() end) end)
    end
    self.Text = "  ğŸ± è‡ªå‹• V4 : " .. (_G.AutoV4 and "ON" or "OFF")
end)

Tab2:AddButton("è²“é€è¦– (ESP) : OFF", function(self)
    _G.ESP = not _G.ESP
    if _G.ESP and not Loaded.ESP then
        Loaded.ESP = true
        task.spawn(function() pcall(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOESP"))() end) end)
    end
    self.Text = "  ğŸ± è²“é€è¦– (ESP) : " .. (_G.ESP and "ON" or "OFF")
end)

