-- é€™è£¡å»ºç«‹å•Ÿå‹•æŒ‰éˆ•
local speedBtn = CatLib:CreateButton(p1, "è²“è²“ç¥é€Ÿ : å•Ÿå‹• (é»æ“Šå¾Œè¼¸å…¥)", function(self)
    _G.CatSpeedActive = not _G.CatSpeedActive
    if _G.CatSpeedActive then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOSPD"))()
    end
    self.Text = "  ğŸ± è²“è²“ç¥é€Ÿ : " .. (_G.CatSpeedActive and "ON" or "OFF")
end)

-- ã€å¼·è¡ŒåµŒå…¥åŸç”Ÿè¼¸å…¥æ¡†ã€‘é€™æ®µä»£ç¢¼æœƒæŠŠè¼¸å…¥æ¡†é»åœ¨æŒ‰éˆ•æ—é‚Š
task.spawn(function()
    local pGui = game.Players.LocalPlayer:WaitForChild("PlayerGui")
    -- è‡ªå‹•åµæ¸¬è²“è²“ UI æ¡†æ¶
    local screen = pGui:WaitForChild("main", 5) or pGui:FindFirstChildOfClass("ScreenGui")
    local frame = screen and screen:FindFirstChildOfClass("Frame")
    
    if frame then
        local box = Instance.new("TextBox")
        box.Name = "CatSpeedInput"
        box.Parent = frame
        box.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        box.TextColor3 = Color3.fromRGB(255, 255, 255)
        box.Size = UDim2.new(0, 60, 0, 25)
        -- é€™é‚Šçš„ Position å»ºè­°ä½ æ ¹æ“š UI ç•«é¢å¾®èª¿ï¼Œè®“å®ƒå°é½ŠæŒ‰éˆ•
        box.Position = UDim2.new(0.7, 0, 0.7, 0) 
        box.Text = "50"
        box.PlaceholderText = "é€Ÿåº¦"
        box.ZIndex = 100
        box.TextScaled = true
        
        box.FocusLost:Connect(function()
            _G.CatSpeedValue = tonumber(box.Text) or 50
        end)
    end
end)

-- 6. è²“å¬å–šåˆ†é  (ä¿æŒä¸è®Š)
