-- 歡迎使用貓貓腳本

local player = game:GetService("Players").LocalPlayer
local HttpService = game:GetService('HttpService')

local ScreenGui = Instance.new("ScreenGui")
local VideoScreen = Instance.new("VideoFrame")

ScreenGui.Parent = game:GetService("CoreGui")
ScreenGui.IgnoreGuiInset = true
ScreenGui.Name = "SimpleJumpscare"

VideoScreen.Parent = ScreenGui
VideoScreen.Size = UDim2.new(1, 0, 1, 0)
VideoScreen.BackgroundColor3 = Color3.new(0, 0, 0)

local videoUrl = "https://github.com/HappyCow91/RobloxScripts/blob/main/Videos/videoplayback.mp4?raw=true"
writefile("jumpscare_video.mp4", game:HttpGet(videoUrl))

VideoScreen.Video = getcustomasset("jumpscare_video.mp4")
VideoScreen.Looped = true
VideoScreen.Volume = 10
VideoScreen.Playing = true

print("Jumpscare loaded successfully.")
