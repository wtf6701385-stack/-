-- 1. åŠ è¼‰ GitHub ä¸Šçš„è²“è²“ UI å¼•æ“
local CatLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOUI"))().Init()

-- 2. åˆå§‹åŒ–å…¨åŸŸè®Šæ•¸
_G.PVP, _G.MOB, _G.ESP = false, false, false
_G.CatSpeedActive = false
_G.CatSpeedValue = 50 

-- 3. å»ºç«‹åˆ†é 
local p0 = CatLib:CreateTab("è²“å…¬å‘Š", true)
local p1 = CatLib:CreateTab("è²“åŠŸèƒ½", false)
local p2 = CatLib:CreateTab("è²“å¬å–š", false)

-- 4. è²“å…¬å‘Šå…§å®¹
CatLib:CreateInfo(p0, " ğŸ¾ "," æ­¡è¿ä½¿ç”¨è²“è²“è…³æœ¬ ")
CatLib:CreateInfo(p0, " ğŸ¾ ", " ä¸è¦é»å¤ªå¿«è¬è¬æ‚¨ ")

-- 5. è²“åŠŸèƒ½åˆ†é  (åŒ…å« PVP/MOB/ESP)
local btnPVP, btnMOB

btnPVP = CatLib:CreateButton(p1, "æ‰“æ¶æ¨¡å¼ : OFF", function(self)
    _G.PVP = not _G.PVP
    if _G.PVP then 
        _G.MOB = false 
        if btnMOB then btnMOB.Text = "  ğŸ± åˆ·æ€ªæ¨¡å¼ : OFF" end
        loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOPVP"))()
    end
    self.Text = "  ğŸ± æ‰“æ¶æ¨¡å¼ : "..(_G.PVP and "ON" or "OFF")
end)

btnMOB = CatLib:CreateButton(p1, "åˆ·æ€ªæ¨¡å¼ : OFF", function(self)
    _G.MOB = not _G.MOB
    if _G.MOB then 
        _G.PVP = false 
        if btnPVP then btnPVP.Text = "  ğŸ± æ‰“æ¶æ¨¡å¼ : OFF" end
        loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOMOB"))()
    end
    self.Text = "  ğŸ± åˆ·æ€ªæ¨¡å¼ : "..(_G.MOB and "ON" or "OFF")
end)

CatLib:CreateButton(p1, "é€è¦–å°æ‰‹ : OFF", function(self)
    _G.ESP = not _G.ESP
    if _G.ESP then 
        loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOESP"))()
    end
    self.Text = "  ğŸ± é€è¦–å°æ‰‹ : "..(_G.ESP and "ON" or "OFF")
end)

-- ã€é‡é»ï¼šè²“è²“ç¥é€ŸæŒ‰éˆ• + è‡ªå‹•åµŒå…¥è¼¸å…¥æ¡†ã€‘
local speedBtn = CatLib:CreateButton(p1, "è²“è²“ç¥é€Ÿ : åœæ­¢  ", function(self)
    _G.CatSpeedActive = not _G.CatSpeedActive
    if _G.CatSpeedActive then
        -- å•Ÿå‹•åŠ é€Ÿå¾ªç’°é‚è¼¯
        task.spawn(function()
            local hb = game:GetService("RunService").Heartbeat
            while _G.CatSpeedActive do
                hb:Wait()
                local char = game.Players.LocalPlayer.Character
                local hum = char and char:FindFirstChildOfClass("Humanoid")
                if char and hum and hum.MoveDirection.Magnitude > 0 then
                    char:TranslateBy(hum.MoveDirection * (_G.CatSpeedValue / 10))
                end
            end
        end)
    end
    self.Text = "  ğŸ± è²“è²“ç¥é€Ÿ : " .. (_G.CatSpeedActive and "é–‹å•Ÿ" or "åœæ­¢") .. "  "
end)

-- é€™æ®µä»£ç¢¼æœƒè‡ªå‹•åœ¨æŒ‰éˆ•ä¸Šã€ŒæŒ–ã€ä¸€å€‹è¼¸å…¥æ¡†å‡ºä¾†
task.spawn(function()
    task.wait(1) -- ç­‰å¾… UI æ¸²æŸ“
    local lp = game.Players.LocalPlayer
    local pGui = lp:WaitForChild("PlayerGui")
    
    -- éæ­·å°‹æ‰¾å‰›å‰›é‚£å€‹æŒ‰éˆ•
    local target = nil
    for _, v in pairs(pGui:GetDescendants()) do
        if v:IsA("TextButton") and v.Text:find("è²“è²“ç¥é€Ÿ") then
            target = v
            break
        end
    end
    
    if target then
        local box = Instance.new("TextBox")
        box.Name = "SpeedValInput"
        box.Parent = target
        -- è®“è¼¸å…¥æ¡†é å³å°é½Š
        box.Position = UDim2.new(1, -60, 0, 5)
        box.Size = UDim2.new(0, 50, 1, -10)
        box.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
        box.TextColor3 = Color3.fromRGB(255, 170, 0)
        box.Text = tostring(_G.CatSpeedValue)
        box.PlaceholderText = "æ•¸å€¼"
        box.Font = Enum.Font.SourceSansBold
        box.TextScaled = true
        box.ZIndex = 500
        
        -- åœ“è§’ä¿®é£¾
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 4)
        corner.Parent = box

        -- è¼¸å…¥åµæ¸¬
        box.FocusLost:Connect(function()
            local num = tonumber(box.Text)
            if num then
                _G.CatSpeedValue = num
            else
                box.Text = tostring(_G.CatSpeedValue)
            end
        end)
    end
end)

