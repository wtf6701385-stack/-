local CatLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/NEKOUI"))().Init()

-- 2. åˆå§‹åŒ–å…¨åŸŸè®Šæ•¸
_G.PVP, _G.MOB, _G.ESP = false, false, false
_G.CatSpeedActive = false
_G.CatSpeedValue = 50 -- é è¨­é€Ÿåº¦

-- 3. å»ºç«‹åˆ†é 
local p0 = CatLib:CreateTab("è²“å…¬å‘Š", true)
local p1 = CatLib:CreateTab("è²“åŠŸèƒ½", false)
