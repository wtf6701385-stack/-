-- [[ ğŸ¾ è²“è²“è…³æœ¬ (CAT SCRIPT) - NEKO TERMINAL ğŸ¾ ]] --
-- æ ¸å¿ƒé‚è¼¯ï¼šè²“è²“è£½ä½œ
-- UI æ¡†æ¶ï¼šè²“è²“å°ˆå±¬ UI (å°æŸ’å„ªåŒ–ç‰ˆ)
-- é©é…åŸ·è¡Œå™¨ï¼šDelta, Hydrogen, Fluxus

local library = {}
local services = setmetatable({}, {__index = function(t, k) return game:GetService(k) end})
local lp = services.Players.LocalPlayer
local run = services.RunService

-- ã€ ğŸ¨ è²“è²“ä¸»é¡Œé…ç½® ã€‘
local Theme = {
    Main = Color3.fromRGB(15, 15, 15),
    Accent = Color3.fromRGB(255, 120, 180), -- è²“å’ªç²‰
    Text = Color3.fromRGB(255, 255, 255),
    Secondary = Color3.fromRGB(30, 30, 35),
    Trans = 0.4
}

-- ã€ âš™ï¸ æ ¸å¿ƒå¾Œå°è®Šæ•¸ ã€‘
getfenv().translateSpeed = 50
getfenv().translateAccelEnabled = false
_G.FastAttack = false
_G.AttackRange = 10000
_G.AutoV4 = false

-- ã€ ğŸ› ï¸ UI æ§‹å»ºå‡½æ•¸ ã€‘
local function Tween(obj, t, data)
    services.TweenService:Create(obj, TweenInfo.new(t, Enum.EasingStyle.Sine), data):Play()
end

function library.new(name)
    local sg = Instance.new("ScreenGui", services.CoreGui)
    sg.Name = "CatScript_UI"
    sg.ResetOnSpawn = false

    local Main = Instance.new("Frame", sg)
    Main.Size = UDim2.new(0, 450, 0, 280)
    Main.Position = UDim2.new(0.5, -225, 0.5, -140)
    Main.BackgroundColor3 = Theme.Main
    Main.BackgroundTransparency = Theme.Trans
    Main.BorderSizePixel = 0
    Instance.new("UICorner", Main).CornerRadius = UDim.new(0, 10)

    -- é ‚éƒ¨éœ“è™¹ç²‰é£¾æ¢
    local Line = Instance.new("Frame", Main)
    Line.Size = UDim2.new(1, 0, 0, 2)
    Line.BackgroundColor3 = Theme.Accent
    Line.BorderSizePixel = 0

    -- å³ä¸Šè§’é–‹é—œæŒ‰éˆ• (ğŸ¾)
    local OpenBtn = Instance.new("TextButton", sg)
    OpenBtn.Size = UDim2.new(0, 45, 0, 45)
    OpenBtn.Position = UDim2.new(0, 10, 0.5, -22)
    OpenBtn.BackgroundColor3 = Theme.Main
    OpenBtn.BackgroundTransparency = 0.2
    OpenBtn.Text = "ğŸ¾"
    OpenBtn.TextColor3 = Theme.Accent
    OpenBtn.TextSize = 25
    Instance.new("UICorner", OpenBtn).CornerRadius = UDim.new(1, 0)
    OpenBtn.MouseButton1Click:Connect(function() 
        Main.Visible = not Main.Visible 
        Tween(OpenBtn, 0.3, {Rotation = Main.Visible and 360 or 0})
    end)

    local Side = Instance.new("Frame", Main)
    Side.Size = UDim2.new(0, 110, 1, 0)
    Side.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
    Side.BackgroundTransparency = 0.3
    
    local Title = Instance.new("TextLabel", Side)
    Title.Size = UDim2.new(1, 0, 0, 45); Title.Text = name; Title.TextColor3 = Theme.Accent; Title.Font = "GothamBold"; Title.TextSize = 15; Title.BackgroundTransparency = 1

    local TabContainer = Instance.new("ScrollingFrame", Side)
    TabContainer.Size = UDim2.new(1, 0, 1, -50); TabContainer.Position = UDim2.new(0, 0, 0, 45); TabContainer.BackgroundTransparency = 1; TabContainer.ScrollBarThickness = 0
    local TabList = Instance.new("UIListLayout", TabContainer); TabList.Padding = UDim.new(0, 5); TabList.HorizontalAlignment = "Center"

    local PageContainer = Instance.new("Frame", Main)
    PageContainer.Size = UDim2.new(1, -125, 1, -15); PageContainer.Position = UDim2.new(0, 120, 0, 8); PageContainer.BackgroundTransparency = 1

    local window = { currentTab = nil }

    function window.Tab(tabName)
        local Page = Instance.new("ScrollingFrame", PageContainer)
        Page.Size = UDim2.new(1, 0, 1, 0); Page.BackgroundTransparency = 1; Page.Visible = false; Page.ScrollBarThickness = 0
        Instance.new("UIListLayout", Page).Padding = UDim.new(0, 8)

        local TabBtn = Instance.new("TextButton", TabContainer)
        TabBtn.Size = UDim2.new(0.9, 0, 0, 32); TabBtn.BackgroundColor3 = Theme.Secondary; TabBtn.Text = tabName; TabBtn.TextColor3 = Color3.fromRGB(150, 150, 150); TabBtn.Font = "GothamMedium"; TabBtn.TextSize = 11; Instance.new("UICorner", TabBtn)

        TabBtn.MouseButton1Click:Connect(function()
            if window.currentTab then window.currentTab.Page.Visible = false; window.currentTab.Btn.TextColor3 = Color3.fromRGB(150, 150, 150) end
            Page.Visible = true; TabBtn.TextColor3 = Theme.Accent
            window.currentTab = {Page = Page, Btn = TabBtn}
        end)

        if not window.currentTab then Page.Visible = true; TabBtn.TextColor3 = Theme.Accent; window.currentTab = {Page = Page, Btn = TabBtn} end

        local tab = {}
        function tab.Section(secName)
            local Section = Instance.new("Frame", Page)
            Section.Size = UDim2.new(0.96, 0, 0, 35); Section.BackgroundColor3 = Theme.Secondary; Section.BackgroundTransparency = 0.5; Instance.new("UICorner", Section)
            local SecTitle = Instance.new("TextLabel", Section)
            SecTitle.Size = UDim2.new(1, -10, 0, 25); SecTitle.Position = UDim2.new(0, 10, 0, 0); SecTitle.Text = secName; SecTitle.TextColor3 = Theme.Accent; SecTitle.Font = "GothamBold"; SecTitle.TextSize = 12; SecTitle.TextXAlignment = "Left"; SecTitle.BackgroundTransparency = 1
            local Container = Instance.new("Frame", Section); Container.Position = UDim2.new(0, 0, 0, 28); Container.Size = UDim2.new(1, 0, 0, 0); Container.BackgroundTransparency = 1
            local CL = Instance.new("UIListLayout", Container); CL.Padding = UDim.new(0, 5); CL.HorizontalAlignment = "Center"
            CL:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function() Section.Size = UDim2.new(0.96, 0, 0, CL.AbsoluteContentSize.Y + 35) end)

            local el = {}
            function el.Button(txt, cb)
                local b = Instance.new("TextButton", Container); b.Size = UDim2.new(0.92, 0, 0, 30); b.BackgroundColor3 = Color3.fromRGB(40, 40, 45); b.Text = txt; b.TextColor3 = Theme.Text; b.Font = "Gotham"; b.TextSize = 11; Instance.new("UICorner", b); b.MouseButton1Click:Connect(cb)
            end
            function el.Toggle(txt, cb)
                local b = Instance.new("TextButton", Container); b.Size = UDim2.new(0.92, 0, 0, 30); b.BackgroundColor3 = Color3.fromRGB(35, 35, 40); b.Text = "  "..txt; b.TextColor3 = Theme.Text; b.Font = "Gotham"; b.TextSize = 11; b.TextXAlignment = "Left"; Instance.new("UICorner", b)
                local s = Instance.new("Frame", b); s.Size = UDim2.new(0, 30, 0, 14); s.Position = UDim2.new(1, -10, 0.5, 0); s.AnchorPoint = Vector2.new(1, 0.5); s.BackgroundColor3 = Color3.fromRGB(60, 60, 60); Instance.new("UICorner", s).CornerRadius = UDim.new(1, 0)
                local t = false; b.MouseButton1Click:Connect(function() t = not t; Tween(s, 0.2, {BackgroundColor3 = t and Theme.Accent or Color3.fromRGB(60, 60, 60)}); cb(t) end)
            end
            function el.Textbox(txt, hold, cb)
                local f = Instance.new("Frame", Container); f.Size = UDim2.new(0.92, 0, 0, 30); f.BackgroundColor3 = Color3.fromRGB(30, 30, 35); Instance.new("UICorner", f)
                local l = Instance.new("TextLabel", f); l.Size = UDim2.new(0.5, 0, 1, 0); l.Position = UDim2.new(0, 10, 0, 0); l.Text = txt; l.TextColor3 = Theme.Text; l.TextSize = 10; l.Font = "Gotham"; l.TextXAlignment = "Left"; l.BackgroundTransparency = 1
                local i = Instance.new("TextBox", f); i.Size = UDim2.new(0.4, 0, 0, 20); i.Position = UDim2.new(1, -5, 0.5, 0); i.AnchorPoint = Vector2.new(1, 0.5); i.BackgroundColor3 = Color3.fromRGB(20, 20, 25); i.Text = ""; i.PlaceholderText = hold; i.PlaceholderColor3 = Color3.fromRGB(100,100,100); i.TextColor3 = Theme.Accent; i.Font = "Gotham"; i.TextSize = 10; Instance.new("UICorner", i); i.FocusLost:Connect(function() cb(i.Text) end)
            end
            return el
        end
        return tab
    end
    return window
end

-- ==========================================
-- ã€ ğŸˆ è²“è²“è…³æœ¬ åŠŸèƒ½è£è¼‰ ã€‘
-- ==========================================
local myUI = library.new("è²“è²“è…³æœ¬")

local Tab1 = myUI.Tab("å…¬å‘Š")
local Info = Tab1.Section("è²“è²“è£½ä½œ")
Info.Button("ğŸ¾ è²“è²“è£½ä½œ - å°ˆå±¬çµ‚ç«¯", function() end)
Info.Button("ğŸ¾ è…³æœ¬æ¸¬è©¦ä¸­ - è²“è²“å‡ºå“", function() end)

local Tab2 = myUI.Tab("æˆ°é¬¥")
local Combat = Tab2.Section("è²“è²“ç§’æ®º")
Combat.Toggle("ç§’æ®ºé–‹é—œ", function(state) _G.FastAttack = state end)
Combat.Textbox("ç§’æ®ºç¯„åœ", "10000", function(val) _G.AttackRange = tonumber(val) or 10000 end)
Combat.Toggle("è‡ªå‹•V4è¦ºé†’", function(state) _G.AutoV4 = state end)

local Tab3 = myUI.Tab("ç§»å‹•")
local Move = Tab3.Section("è²“è²“åŠ é€Ÿ")
Move.Textbox("é€Ÿåº¦å€¼", "50", function(val) getfenv().translateSpeed = tonumber(val) or 50 end)
Move.Toggle("åŠ é€Ÿé–‹é—œ", function(state) getfenv().translateAccelEnabled = state end)

-- ==========================================
-- ã€ ğŸš€ å¾Œå°é‚è¼¯åŸ·è¡Œ ã€‘
-- ==========================================

-- 1. é˜²æ›æ©Ÿ (è²“è²“é˜²æ–·ç·š)
services.Players.LocalPlayer.Idled:Connect(function()
    services.VirtualUser:CaptureController()
    services.VirtualUser:ClickButton2(Vector2.new(0,0))
    print("è²“è²“è…³æœ¬ | å·²æ””æˆªæ›æ©Ÿæª¢æ¸¬å–µ~")
end)

-- 2. åŠ é€ŸåŸ·è¡Œ
run.Heartbeat:Connect(function()
    if getfenv().translateAccelEnabled and lp.Character and lp.Character:FindFirstChild("Humanoid") then
        local hum = lp.Character.Humanoid
        if hum.MoveDirection.Magnitude > 0 then
            lp.Character:TranslateBy(hum.MoveDirection * (getfenv().translateSpeed / 30))
        end
    end
end)

-- 3. è‡ªå‹• V4 (è²“è²“è¦ºé†’)
task.spawn(function()
    while true do
        if _G.AutoV4 then
            pcall(function()
                local awake = lp.Backpack:FindFirstChild("Awakening") or lp.Character:FindFirstChild("Awakening")
                if awake then awake.RemoteFunction:InvokeServer(true) end
            end)
        end
        task.wait(1)
    end
end)

services.StarterGui:SetCore("SendNotification", {
    Title = "ğŸ¾ è²“è²“è…³æœ¬",
    Text = "è²“è²“è£½ä½œï¼Œè¼‰å…¥å®Œæˆå–µ~",
    Duration = 5
})
