local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")
local LocalPlayer = Players.LocalPlayer

-- 1. éŠæˆ²åµæ¸¬é‚è¼¯
local GameList = {
    [994732206]  = "e4aedc7ccd2bacd83555baa884f3d4b1", -- Blox Fruit
    [5750914919] = "b94343ca266a778e5da8d72e92d4aab5", -- Fisch
    [4777817887] = "35ad587b07c00b82c218fcf0e55eeea6"  -- Blade Ball
}
local scriptId = GameList[game.GameId] or _G.loadCustomId

if not scriptId then
    loadstring(game:HttpGet('https://raw.githubusercontent.com/acsu123/HohoV2/refs/heads/main/ScriptLoadButOlder.lua'))()
    return
end

-- 2. è¼‰å…¥ Luarmor API
local api = loadstring(game:HttpGet("https://sdkapi-public.luarmor.net/library.lua"))()
api.script_id = scriptId

-- 3. è¼‰å…¥åˆ†é›¢å‡ºçš„ UI æ¨¡çµ„ (é€™è£¡æ”¹ç‚ºä½  GitHub çš„ä½å­)
-- âš ï¸ è«‹å°‡ä¸‹æ–¹çš„ç¶²å€æ›¿æ›æˆä½ ä¸Šå‚³ UI.lua å¾Œçš„ RAW é€£çµ
local UI_Loader = loadstring(game:HttpGet("YOUR_UI_GITHUB_LINK"))()
local UI = UI_Loader.CreateUI() -- å‘¼å« UI ç”Ÿæˆå‡½æ•¸

-- 4. åŠŸèƒ½å°æ¥ (Logic Linking)
UI.SubmitBtn.MouseButton1Click:Connect(function()
    local key = UI.Textbox.Text
    key = key:gsub("[%s\t\r\n]", "")
    
    local status = api.check_key(key)
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "NEKO Key System",
        Text = status.message
    })
    
    if status.code == "KEY_VALID" then
        writefile("HohoKeyV4.txt", key)
        UI.Destroy() -- é©—è­‰æˆåŠŸå¾Œé—œé–‰ UI
        api.load_script()
    end
end)

UI.GetBtn.MouseButton1Click:Connect(function()
    setclipboard('https://hehehub-acsu123.pythonanywhere.com/api/getkey?hwid=' .. tick())
end)

-- 5. è‡ªå‹•è®€å–èˆŠ Key
if isfile("HohoKeyV4.txt") then
    local savedKey = readfile("HohoKeyV4.txt")
    UI.Textbox.Text = savedKey
    -- è‡ªå‹•åŸ·è¡Œä¸€æ¬¡æª¢æŸ¥
end

print("ğŸ¾ NEKO æ ¸å¿ƒè¼‰å…¥å®Œæˆï¼Œç­‰å¾… Key é©—è­‰...")
