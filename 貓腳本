-- [[ ğŸ¾ NEKO TERMINAL x HOHO - MAIN LOADER ğŸ¾ ]] --

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local StarterGui = game:GetService("StarterGui")

-- ğŸš€ 1. éŠæˆ²ç’°å¢ƒåµæ¸¬ (ç¢ºä¿ ID èˆ‡ Luarmor å°æ¥)
local GameList = {
    [994732206]  = "e4aedc7ccd2bacd83555baa884f3d4b1", -- Blox Fruit
    [7018190066] = "bf149e75708e91ad902bd72e408fae02", -- Dead Rails
    [383310974]  = "b83e9255dc81e9392da975a89d26e363", -- Adopt Me
    [4777817887] = "35ad587b07c00b82c218fcf0e55eeea6", -- Blade Ball
    [5750914919] = "b94343ca266a778e5da8d72e92d4aab5", -- Fisch
    [5361032378] = "ff4e04500b94246eaa3f5c5be92a8b4a", -- Sol's RNG
}

local scriptId = GameList[game.GameId] or _G.loadCustomId

-- âš ï¸ å¦‚æœä¸åœ¨æ”¯æ´æ¸…å–®ï¼Œè¼‰å…¥èˆŠç‰ˆ
if not scriptId then
    loadstring(game:HttpGet('https://raw.githubusercontent.com/acsu123/HohoV2/refs/heads/main/ScriptLoadButOlder.lua'))()
    return
end

-- ğŸ”‘ 2. åˆå§‹åŒ– Luarmor SDK
local api = loadstring(game:HttpGet("https://sdkapi-public.luarmor.net/library.lua"))()
api.script_id = scriptId

-- ğŸ¨ 3. è¼‰å…¥ä¸»äººå°è£å¥½çš„ UI
-- é€™è£¡æœƒç”Ÿæˆ HOHO_Passcheck ç­‰ç‰©ä»¶
loadstring(game:HttpGet("https://raw.githubusercontent.com/wtf6701385-stack/-/refs/heads/main/Ok"))()

-- ğŸ•µï¸ 4. ç²å– UI ç‰©ä»¶å°æ¥é» (éœ€ç¢ºä¿ UI è…³æœ¬å…§ç‰©ä»¶åç¨±ä¸€è‡´)
local CoreGui = game:GetService("CoreGui")
local PassGui = CoreGui:WaitForChild("HĞ¾hĞ¾_Ñ€Ğ°ssÑhĞµÑk") -- æ ¹æ“šä¸»äººæä¾›çš„ UI å‘½å
local MainFrame = PassGui:WaitForChild("GET_KEY")
local SubmitBtn = MainFrame:WaitForChild("Submit")
local GetKeyBtn = MainFrame:WaitForChild("Get")
local TextBox = MainFrame:WaitForChild("Frame"):WaitForChild("Textbox")

-- ğŸ§  5. æ ¸å¿ƒé©—è­‰é‚è¼¯å°æ¥
local function DoCheckKey(key)
    key = key:gsub("[%s\t\r\n]", "")
    local status = api.check_key(key)

    StarterGui:SetCore("SendNotification", {
        Title = "NEKO Key System",
        Text = "[".. status.code .. "] " .. status.message,
        Duration = 5
    })

    if status.code == "KEY_VALID" then
        writefile("HohoKeyV4.txt", key)
        _G.script_key = key -- å…¨åŸŸå„²å­˜ Key
        
        -- é—œé–‰ UI ä¸¦åŸ·è¡Œå¾ŒçºŒåŠŸèƒ½
        PassGui:Destroy()
        api.load_script()
    end
end

-- ğŸ–±ï¸ 6. æŒ‰éˆ•äº‹ä»¶ç¶å®š
SubmitBtn.MouseButton1Click:Connect(function()
    DoCheckKey(TextBox.Text)
end)

GetKeyBtn.MouseButton1Click:Connect(function()
    setclipboard('https://hehehub-acsu123.pythonanywhere.com/api/getkey?hwid=' .. tick())
    StarterGui:SetCore("SendNotification", {
        Title = "Key System",
        Text = "Key Link Copied!",
        Duration = 3
    })
end)

-- ğŸ”„ 7. è‡ªå‹•è®€å–èˆŠ Key é‚è¼¯
if isfile("HohoKeyV4.txt") then
    local savedKey = readfile("HohoKeyV4.txt")
    TextBox.Text = savedKey
    DoCheckKey(savedKey)
end

print("ğŸ¾ NEKO x HOHO å°æ¥å®Œæˆï¼Œå…¨ç³»çµ±ç›£æ§ä¸­å–µï¼")
